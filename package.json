{
  "name": "zen-lang",
  "version": "0.2.0",
  "description": "A markdown extension language for multi-agent systems",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "zen": "dist/src/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "npm run build && node dist/tests/parser.test.js && node dist/tests/compiler.test.js && node dist/tests/integration.test.js && node dist/tests/stress/edge-cases.test.js && node dist/tests/v02-features.test.js",
    "test:parser": "npm run build && node dist/tests/parser.test.js",
    "test:compiler": "npm run build && node dist/tests/compiler.test.js",
    "test:integration": "npm run build && node dist/tests/integration.test.js",
    "test:stress": "npm run build && node dist/tests/stress/edge-cases.test.js",
    "test:v02": "npm run build && node dist/tests/v02-features.test.js",
    "cli": "npm run build && node dist/src/cli/index.js"
  },
  "keywords": [
    "markdown",
    "language",
    "multi-agent",
    "orchestration",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.3.0",
    "@types/node": "^20.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
