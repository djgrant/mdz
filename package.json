{
  "name": "zenmarkdown",
  "version": "0.3.0",
  "description": "A markdown extension language for multi-agent systems",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "mdz": "dist/src/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "pnpm build && node dist/tests/parser.test.js && node dist/tests/compiler.test.js && node dist/tests/integration.test.js && node dist/tests/stress/edge-cases.test.js && node dist/tests/v02-features.test.js",
    "test:parser": "pnpm build && node dist/tests/parser.test.js",
    "test:compiler": "pnpm build && node dist/tests/compiler.test.js",
    "test:integration": "pnpm build && node dist/tests/integration.test.js",
    "test:stress": "pnpm build && node dist/tests/stress/edge-cases.test.js",
    "test:v02": "pnpm build && node dist/tests/v02-features.test.js",
    "cli": "pnpm build && node dist/src/cli/index.js",
    "web": "pnpm -F zen-website --"
  },
  "keywords": [
    "markdown",
    "language",
    "multi-agent",
    "orchestration",
    "llm"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.3.0",
    "@types/node": "^20.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp"
    ]
  }
}
